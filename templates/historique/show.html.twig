{% extends 'base_admin.html.twig' %}

{% block title %}Historique{% endblock %}

{% block body %}
    <h1>Historique</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ historique.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ historique.Libelle }}</td>
            </tr>
        </tbody>
    </table> 

    <a href="{{ path('historique_index') }}"><span class="btn btn-info">Retour</span></a>

    <a href="{{ path('historique_edit', {'id': historique.id}) }}" class="btn btn-primary"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>

    {{ include('historique/_delete_form.html.twig') }}
{% endblock %}
