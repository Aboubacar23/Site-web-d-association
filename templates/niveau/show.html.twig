{% extends 'base_admin.html.twig' %}

{% block title %}Niveau{% endblock %}

{% block body %}
    <h1>Niveau</h1>

    <table class="table table-bordered table-hover table-primary">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ niveau.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ niveau.Libelle }}</td>
