<form method="post" action="{{ path('register_delete', {'id': admin.id}) }}" 
onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ admin.id) }}">
    <button class="btn btn-danger fa fa-trash-o fa-lg">Delete</button>
</form>
 